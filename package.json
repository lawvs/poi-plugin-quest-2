    "pangu": "^4.0.7",
    "poi-asset-themes": "^4.5.0",
    "prettier": "^3.3.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^11.8.5",
    "sharp": "^0.33.5",
    "storybook": "^7.5.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^4.7.4",
    "undici": "^6.19.8"
